    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^1.14.1"
